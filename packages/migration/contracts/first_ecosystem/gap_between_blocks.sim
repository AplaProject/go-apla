// +prop AppID = 2
// +prop Conditions = 'ContractConditions("MainCondition")'
contract gap_between_blocks {
      data {
          Value string
      }
  
      conditions {
          if Size($Value) == 0 {
              warning "Value was not received"
          }
          if Int($Value) <= 0 || Int($Value) >= 86400 {
              warning "Value must be between 1 and 86399"
          }
      }
}