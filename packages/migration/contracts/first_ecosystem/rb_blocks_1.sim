// +prop AppID = 2
// +prop Conditions = 'ContractConditions("MainCondition")'
contract rb_blocks_1 {
      data {
          Value string
      }
  
      conditions {
          if Size($Value) == 0 {
              warning "Value was not received"
          }
          if Int($Value) < 1 || Int($Value) > 999 {
              warning "Value must be between 1 and 999"
          }
      }
}